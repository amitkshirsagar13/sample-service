pipeline {
  agent any
  tools {
    maven 'maven'
  }
  stages {
    stage('Git Checkout') {
      steps {
        script {
          clone('https://github.com/amitkshirsagar13/sample-service.git')
        }
      }
    }
    stage('Build') {
      steps {
        script {
          build()
        }
      }
    }
    stage('Deploy') {
      steps {
        script {
          deploy_to dev
          deploy_to test
        }
      }
    }
  }
}
